<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ninespace.sqlite.mapper.UserMapper">

    <select id="selectByAddr" resultType="com.ninespace.sqlite.entity.User">
        SELECT
        *
        FROM
        t_user u
        WHERE
        <bind name="tag" value="addr + '%'"/>
        u.addr LIKE #{tag}
    </select>


</mapper>
